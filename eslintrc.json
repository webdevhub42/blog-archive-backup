{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": ["prettier", "javascriptreact"],
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "no-unused-vars": 0
    }
}
